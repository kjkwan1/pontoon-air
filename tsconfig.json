{
    "compilerOptions": {
      "target": "es5",
      "module": "es2020",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,
      "outDir": "./dist"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
